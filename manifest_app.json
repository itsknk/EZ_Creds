{
  "name": "EZ Creds",
  "version": "0.0",
  "manifest_version": 2,
  "description": "Generate passwords for websites based on one master password and their domains.",
  "icons": {
    "16": "icons/16x16.png",
    "19": "icons/19x19.png",
    "32": "icons/32x32.png",
  },
  "permissions": ["storage"],
  "offline_enabled": true,
  "app": {
    "background": {
      "scripts": ["js/background.js", "js/storage.js"]
    }
  }
}
